<h1> Links </h1>

<ul id='links'>
  <% @links.each do |link| %>
  <li>
    Title: <%= link.title %>
    URL: <%= link.url %>
    Tags: <%= link.tags %>
  </li>
  <% end %>
</ul>

<form method='get' action='/links'>
  <div>
    <input type="submit" value="Back to all links" id="add_link">
  </div>
</form>
